#include<bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.setf(ios::fixed,ios::floatfield);

    long long int t, n, j=0;

    cin>>t;
    while(t--)
    {
        cin>>n;
        long long int arr[n], i, sum=0;
        for(i=0; i<n; i++)
            cin>>arr[i];
        for(i=0; i<n; i++)
            if(arr[i]>0) sum = sum + arr[i];

        cout<<"Case "<<++j<<": "<<sum<<endl;
    }

    return 0;
}
